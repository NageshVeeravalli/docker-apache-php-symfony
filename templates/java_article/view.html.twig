{% extends 'base.html.twig' %}

{% block page_title %}Java Article{% endblock %}

{% block page_content %}
    <table class="table">
        <tbody>
            <tr>
                <th>Title</th>
                <td>{{ java_article.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ java_article.description }}</td>
            </tr>
        </tbody>
    </table>

    <div class="row">
        <div class="col-md-6">
            {{ form(form) }}
        </div>
    </div>
{% endblock %}
